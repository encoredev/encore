import { apiCall } from "encore.dev/internal/codegen/api";

const TEST_ENDPOINTS = !ENCORE_DROP_TESTS && process.env.NODE_ENV === "test"
    ? await import("./endpoints_testing.js")
    : null;

{{#each endpoints}}
export async function {{name}}(params) {
    if (!ENCORE_DROP_TESTS && process.env.NODE_ENV === "test") {
        return TEST_ENDPOINTS.{{name}}(params);
    }
    return apiCall("{{../name}}", "{{name}}", params);
}

{{/each}}
