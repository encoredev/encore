import { registerHandlers, run, type Handler } from "encore.dev/internal/codegen/appinit";
{{#each endpoints}}
import { {{name}} as {{name}}Impl } from {{toJSON import_path}};
{{/each}}
{{#each subscriptions}}
import {{toJSON import_path}};
{{/each}}

const handlers: Handler[] = [
{{#each endpoints}}
    {
        service:   {{toJSON ../name}},
        name:      {{toJSON name}},
        handler:   {{name}}Impl,
        raw:       {{toJSON raw}},
        streaming: {{toJSON streaming}},
    },
{{/each}}
];

registerHandlers(handlers);
await run();
