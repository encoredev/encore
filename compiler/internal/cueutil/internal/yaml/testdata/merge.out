// From http://yaml.org/type/merge.html
// Test
anchors: {
	list: [{
		x: 1, y: 2
	}, {
		x: 0, y: 2
	}, {
		r: 10
	}, {
		r: 1
	}]
}

// All the following maps are equal:

plain: {
	// Explicit keys
	x:     1
	y:     2
	r:     10
	label: "center/big"
}

mergeOne: {
	x: 1
	y: 2
	// Merge one map

	r:     10
	label: "center/big"
}

mergeMultiple: {
	r: 10
	x: 1
	y: 2
	// Merge multiple maps

	label: "center/big"
}

override: {
	r:     10
	x:     1
	y:     2
	label: "center/big"
}

shortTag: {
	r: 10
	x: 1
	y: 2
	// Explicit short merge tag

	label: "center/big"
}

longTag: {
	r: 10
	x: 1
	y: 2
	// Explicit merge long tag

	label: "center/big"
}

inlineMap: {
	// Inlined map
	x:     1, y: 2, r: 10
	label: "center/big"
}

inlineSequenceMap: {
	// Inlined map in sequence
	r:     10
	x:     1
	y:     2
	label: "center/big"
}